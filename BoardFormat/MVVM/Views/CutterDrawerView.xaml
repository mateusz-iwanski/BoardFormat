<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BoardFormat.MVVM.Views.CutterDrawerView"
             x:Name="view"
             BindingContext="{x:Reference view}"
             >
    <Border>
        <VerticalStackLayout x:Name="GraphicsVerticalStackLayout" Spacing="10" HorizontalOptions="FillAndExpand" HeightRequest="{Binding LayoutHeight}" >
        </VerticalStackLayout>
    </Border>

</ContentView>
